:"","sources":["/Users/rebj/Sites/inyourdaylight/src/components/CarouselText.vue"],"sourcesContent":["<template>\n  <div class=\"carousel-text\">\n    <img class=\"sun\" src=\"../assets/Sun-animated.svg\"/>\n    <transition appear name=\"carousel\">\n        <h1 ref=\"title\" class=\"title\" v-if=\"record\">\n            <span class=\"row1\" ref=\"row\" :key=\"titleCount+record\">\n              <span v-for=\"(item, i) in titleCount\" :key=\"i+titleCount\">{{ record.Title }} </span>\n          </span>\n          <span class=\"row2\">\n              <span v-for=\"(item, i) in titleCount\" :key=\"i+titleCount\">{{ record.Title }} </span>\n          </span>\n        </h1>\n      </transition>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'CarouselText',\n  props: {\n  },\n  watch: {},\n  methods: {\n    animatedTitle() {\n      this.rowWidth = this.$refs.row.getBoundingClientRect().width;\n      this.timeline.to(\".row2\", {x: this.rowWidth,  duration: 0, ease:Linear.easeNone}, 0);\n      this.timeline.to(\".row1\", {x: -((this.rowWidth - window.innerWidth) + window.innerWidth),  duration: 20, ease:Linear.easeNone }, 0);\n      this.timeline.to(\".row2\", {x: 0,  duration: 20, ease:Linear.easeNone }, 0);\n      this.timeline.repeat();\n    }\n  },\n  data() {\n    return {\n      languages: [\n        '한국어',\n        '中文',\n        'Français',\n        'English'\n      ]\n    }\n  },\n  mounted() {\n    this.animatedTitle();\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"scss\" scoped>\n@import \"../styles.scss\";\n.sun {\n    max-height: 100vh;\n    position: absolute;\n    left: 50%;\n    transform: translateX(-50%);\n    z-index: 0;\n}\n.carousel-text {\n    height: 100vh;\n}\n.carousel-enter-active {\n  transition: all 1s ease;\n  opacity: 1;\n}\n.carousel-enter {\n  opacity: 0;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}