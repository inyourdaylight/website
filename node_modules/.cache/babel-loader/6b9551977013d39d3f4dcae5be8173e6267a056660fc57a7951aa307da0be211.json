nts/CarouselText.vue';\nimport ImagesGallery from './components/ImagesGallery.vue';\nimport HeaderNavScreen from './components/HeaderNavScreen.vue';\n\n\nexport default {\n  name: 'App',\n  components: {\n    HeaderNav,\n    FooterNav,\n    HomepageBanner,\n    TextandFadedBG,\n    GoldSquareSection,\n    CarouselText,\n    ImagesGallery,\n    HeaderNavScreen\n  },\n  props: {\n  },\n  watch: {},\n  methods: {\n    scroll() {\n      this.scrollPos = window.scrollY;\n    },\n    clickEvents(e) {\n      switch(e.target.id) {\n        case \"hamburgerMenu\": {\n          this.showNav = !this.showNav;\n          break;\n        }\n      }\n    }\n  },\n  data() {\n    return {\n      scrollLimit: 0,\n      scrollPos: 0,\n      showNav: false\n    }\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.scroll);\n    this.scrollLimit = document.getElementById(\"inner\").getBoundingClientRect().height - window.innerHeight;\n    // Defining only one click event listener here, to prevent possible memory leaks\n    window.addEventListener(\"click\", this.clickEvents);\n  }\n}\n</script>\n\n<style lang=\"scss\">\n@import \"fonts.scss\";\n@import \"styles.scss\";\n.gradient {\n  position: absolute;\n  width: 100vw;\n  height: 479px;\n  background: linear-gradient(180deg, rgba(50, 62, 71, 0) 0%, #323E47 94.79%);\n  transform: rotate(-180deg);\n  z-index: 0;\n  pointer-events: none;\n}\n.page {\n  position: relative;\n  z-index: 2;\n  min-height: 200vh;\n}\n#app {\n  background-color: $darkblue;\n  \n}\nbody {\n    margin: 0;\n    overscroll-behavior-y: none;\n    overscroll-behavior-x: none;\n    width: 100vw;\n    // overflow-x: hidden; \n}\nh1 {\n    font-family: $heading;\n    text-transform: uppercase;\n}\nh3 {\n    font-family: $headinglight;\n}\np, li, div {\n    font-family: $body;\n    color: white;\n    margin-top: 0;\n}\n\n.header-nav-screen  {\n  transition: all 1000ms cubic-bezier(0.355, 0.005, 0.240, 0.990); /* custom */\n  transition-timing-function: cubic-bezier(0.355, 0.005, 0.240, 0.990); /* custom */\n}\n.header-nav-screen-enter-to, .header-nav-screen-leave-active {\n  width: 100vw;\n}\n.header-nav-screen-enter-active, .header-nav-screen-leave-to {\n  width: 0%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}