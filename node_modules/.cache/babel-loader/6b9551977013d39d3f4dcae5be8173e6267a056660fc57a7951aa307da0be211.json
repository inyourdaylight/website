{"ast":null,"code":"import HeaderNav from './components/HeaderNav.vue';\nimport FooterNav from './components/FooterNav.vue';\nimport HomepageBanner from './components/HomepageBanner.vue';\nimport TextandFadedBG from './components/TextandFadedBG.vue';\nimport GoldSquareSection from './components/GoldSquareSection.vue';\nimport CarouselText from './components/CarouselText.vue';\nimport ImagesGallery from './components/ImagesGallery.vue';\nimport HeaderNavScreen from './components/HeaderNavScreen.vue';\nexport default {\n  name: 'App',\n  components: {\n    HeaderNav,\n    FooterNav,\n    HomepageBanner,\n    TextandFadedBG,\n    GoldSquareSection,\n    CarouselText,\n    ImagesGallery,\n    HeaderNavScreen\n  },\n  props: {},\n  watch: {},\n  methods: {\n    scroll() {\n      this.scrollPos = window.scrollY;\n    },\n    clickEvents(e) {\n      switch (e.target.id) {\n        case \"hamburgerMenu\":\n          {\n            this.showNav = !this.showNav;\n            break;\n          }\n      }\n    }\n  },\n  data() {\n    return {\n      scrollLimit: 0,\n      scrollPos: 0,\n      showNav: false\n    };\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.scroll);\n    this.scrollLimit = document.getElementById(\"inner\").getBoundingClientRect().height - window.innerHeight;\n    // Defining only one click event listener here, to prevent possible memory leaks\n    window.addEventListener(\"click\", this.clickEvents);\n  }\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,eAAc,MAAO,kCAAkC;AAG9D,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVT,SAAS;IACTC,SAAS;IACTC,cAAc;IACdC,cAAc;IACdC,iBAAiB;IACjBC,YAAY;IACZC,aAAa;IACbC;EACF,CAAC;EACDG,KAAK,EAAE,CACP,CAAC;EACDC,KAAK,EAAE,CAAC,CAAC;EACTC,OAAO,EAAE;IACPC,MAAM,GAAG;MACP,IAAI,CAACC,SAAQ,GAAIC,MAAM,CAACC,OAAO