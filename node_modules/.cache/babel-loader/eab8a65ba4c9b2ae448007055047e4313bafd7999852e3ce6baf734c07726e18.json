{"ast":null,"code":"import { toRaw } from \"vue\";\nexport default {\n  name: 'PagePage',\n  props: {\n    scrollPos: {\n      type: Number,\n      default: 0\n    },\n    clickEvents: {\n      type: Object,\n      default: () => {}\n    }\n  },\n  components: {},\n  watch: {\n    scrollPos: {\n      handler(scrollPos) {\n        this.transition(scrollPos);\n      }\n      // immediate:true\n    }\n  },\n\n  methods: {\n    transition(scrollPos) {\n      let scrolledComponent = document.getElementById(toRaw(this.componentsIntegered)[this.positions.filter(item => scrollPos >= item).length - 1]);\n      try {\n        if (toRaw(this.positions).filter(item => scrollPos >= item).length > -1 && !scrolledComponent.className.includes(\"fadein\")) {\n          scrolledComponent.className += \" fadein\";\n        }\n      } catch {\n        // No error\n      }\n    }\n  },\n  data() {\n    return {\n      page: null,\n      componentsIntegered: [],\n      positions: []\n    };\n  },\n  mounted() {\n    try {\n      this.page = this.$cms.routes.filter(i => i.path === window.location.pathname)[0];\n      console.log();\n    } catch {\n      console.log(\"page doesn't exist\");\n      window.location.href = \"./404\";\n    }\n    this.componentsIntegered = [...this.page.slices].map((item, i) => item.component + i);\n    this.$nextTick(() => {\n      this.positions = [...this.page.slices].map((id, i) => {\n        let component = document.getElementById(id.component + i);\n        return component.getBoundingClientRect().top - (window.innerHeight - component.getBoundingClientRect().height) / 2 + window.scrollY;\n      });\n\n      // Accounts for if the element already shows on the page\n      this.positions.forEach(pos => {\n        if (pos < window.innerHeight / 3) {\n          this.transition(pos);\n        }\n      });\n      // this.componentsIntegered .forEach(item => {\n      //   document.getElementById(item).className += \" animate-fade-in\";\n      // })\n    });\n  },\n\n  beforeUpdate() {}\n};","map":{"version":3,"mappings":"AAcA,SAAQA,KAAK,QAAQ,KAAK;AAC1B,eAAe;EAEbC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,WAAW,EAAE;MACXH,IAAI,EAAEI,MAAM;MACZF,OAAO,EAAE,MAAM,CAAC;IAClB;EACF,CAAC;EACDG,UAAU,EAAE,CACZ,CAAC;EACDC,KAAK,EAAE;IACLP,SAAS,EAAE;MACTQ,OAAO,CAACR,SAAS,EAAE;QACjB,IAAI,CAACS,UAAU,CAACT,SAAS,CAAC;MAC5B;MACA;IACF;EACF,CAAC;;EACDU,OAAO,EAAE;IACPD,UAAU,CAACT,SAAS,EAAE;MACpB,IAAIW,iBAAgB,GAAIC,QAAQ,CAACC,cAAc,CAAChB,KAAK,CAAC,IAAI,CAACiB,mBAAmB,CAAC,CAAC,IAAI,CAACC,SAAS,CAACC,MAAM,CAACC,IAAG,IAAKjB,SAAQ,IAAKiB,IAAI,CAAC,CAACC,MAAK,GAAI,CAAC,CAAC,CAAC;MAE5I,IAAI;QACH,IAAIrB,KAAK,CAAC,IAAI,CAACkB,SAAS,CAAC,CAACC,MAAM,CAACC,IAAG,IAAKjB,SAAQ,IAAKiB,IAAI,CAAC,CAACC,MAAK,GAAI,CAAC,KAAK,CAACP,iBAAiB,CAACQ,SAAS,CAACC,QAAQ,CAAC,QAAQ,CAAC,EAAE;UAC1HT,iBAAiB,CAACQ,SAAQ,IAAK,SAAS;QAC1C;MACD,EAAE,MAAM;QACP;MAAA;IAEJ;EACF,CAAC;EACDE,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,IAAI;MACVR,mBAAmB,EAAE,EAAE;MACvBC,SAAS,EAAE;IACb;EACF,CAAC;EACDQ,OAAO,GAAG;IACR,IAAI;MACF,IAAI,CAACD,IAAG,GAAI,IAAI,CAACE,IAAI,CAACC,MAAM,CAACT,MAAM,CAACU,KAAKA,CAAC,CAACC,IAAG,KAAMC,MAAM,CAACC,QAAQ,CAACC,QAAQ,CAAC,CAAC,CAAC,CAAC;MAChFC,OAAO,CAACC,GAAG,EAAC;IACd,EAAE,MAAM;MACND,OAAO,CAACC,GAAG,CAAC,oBAAoB,CAAC;MACjCJ,MAAM,CAACC,QAAQ,CAACI,IAAI,GAAC,OAAO;IAC9B;IACA,IAAI,CAACnB,mBAAkB,GAAI,CAAC,GAAG,IAAI,CAACQ,IAAI,CAACY,MAAM,CAAC,CAACC,GAAG,CAAC,CAAClB,IAAI,EAAES,CAAC,KAAKT,IAAI,CAACmB,SAAS,GAACV,CAAC,CAAC;IAEnF,IAAI,CAACW,SAAS,CAAC,MAAM;MACnB,IAAI,CAACtB,SAAQ,GAAI,CAAC,GAAG,IAAI,CAACO,IAAI,CAACY,MAAM,CAAC,CAACC,GAAG,CAAC,CAACG,EAAE,EAACZ,CAAC,KAAK;QACnD,IAAIU,SAAQ,GAAIxB,QAAQ,CAACC,cAAc,CAACyB,EAAE,CAACF,SAAS,GAACV,CAAC,CAAC;QACvD,OAAQU,SAAS,CAACG,qBAAqB,EAAE,CAACC,GAAE,GAAK,CAACZ,MAAM,CAACa,WAAU,GAAIL,SAAS,CAACG,qBAAqB,EAAE,CAACG,MAAM,IAAI,CAAE,GAAId,MAAM,CAACe,OAAO;MACzI,CAAC;;MAED;MACA,IAAI,CAAC5B,SAAS,CAAC6B,OAAO,CAACC,GAAE,IAAK;QAC5B,IAAIA,GAAE,GAAKjB,MAAM,CAACa,WAAU,GAAI,CAAE,EAAE;UAClC,IAAI,CAAChC,UAAU,CAACoC