{"ast":null,"code":"// import {toRaw} from \"vue\";\nexport default {\n  name: 'App',\n  components: {},\n  props: {\n    scrollPos: {\n      type: Number,\n      default: 0\n    }\n  },\n  watch: {\n    scrollPos(scrollPos) {\n      // console.log(\"scrollPos\", scrollPos);\n      if (this.positions.filter(item => scrollPos >= item).length > 0) {\n        document.getElementById(this.components[this.positions.filter(item => scrollPos >= item).length - 1]);\n      }\n    }\n  },\n  methods: {},\n  data() {\n    return {\n      components: ['HomepageBanner', 'TextandFadedBG', 'GoldSquareSection', 'CarouselText', 'ImagesGallery'],\n      positions: []\n    };\n  },\n  mounted() {\n    this.$nextTick(() => {\n      this.positions = [...this.components].map(id => {\n        let component = document.getElementById(id.toLowerCase());\n        return component.getBoundingClientRect().top - (window.innerHeight - component.getBoundingClientRect().height) / 2 + window.scrollY;\n      });\n    });\n  }\n};","map":{"version":3,"mappings":"AAQA;AACA,eAAe;EACbA,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE,CACZ,CAAC;EACDC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,KAAK,EAAE;IACLJ,SAAS,CAACA,SAAS,EAAE;MACnB;MACA,IAAI,IAAI,CAACK,SAAS,CAACC,MAAM,CAACC,IAAG,IAAKP,SAAQ,IAAKO,IAAI,CAAC,CAACC,MAAK,GAAI,CAAC,EAAE;QAC/DC,QAAQ,CAACC,cAAc,CAAC,IAAI,CAACZ,UAAU,CAAC,IAAI,CAACO,SAAS,CAACC,MAAM,CAACC,IAAG,IAAKP,SAAQ,IAAKO,IAAI,CAAC,CAACC,MAAK,GAAI,CAAC,CAAC;MACtG;IACF;EACF,CAAC;EACDG,OAAO,EAAE,CACT,CAAC;EACDC,IAAI,GAAG;IACL,OAAO;MACLd,UAAU,EAAE,CACV,gBAAgB,EAChB,gBAAgB,EAChB,mBAAmB,EACnB,cAAc,EACd,eAAc,CACf;MACDO,SAAS,EAAE;IACb;EACF,CAAC;EACDQ,OAAO,GAAG;IACR,IAAI,CAACC,SAAS,CAAC,MAAM;MACnB,IAAI,CAACT,SAAQ,GAAI,CAAC,GAAG,IAAI,CAACP,UAAU,CAAC,CAACiB,GAAG,CAAEC,EAAE,IAAK;QAChD,IAAIC,SAAQ,GAAIR,QAAQ,CAACC,cAAc,CAACM,EAAE,CAACE,WAAW,EAAE,CAAC;QACzD,OAAQD,SAAS,CAACE,qBAAqB,EAAE,CAACC,GAAE,GAAK,CAACC,MAAM,CAACC,WAAU,GAAIL,SAAS,CAACE,qBAAqB,EAAE,CAACI,MAAM,IAAI,CAAE,GAAIF,MAAM,CAACG,OAAO;MACzI,CAAC;IACH,CAAC;EACH;AACF","names":["name","components","props","scrollPos","type","Number","default","watch","positions","filter","item","length","document","getElementById","methods","data","mounted","$nextTick","map","id","component","toLowerCase","getBoundingClientRect","top","window","innerHeight","height","scrollY"],"sourceRoot":"","sources":["/Users/rebj/Sites/staging-iyd/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"inner\">\n      <component v-for=\"(item, i) in components\" :key=\"i\" :is=\"item\" :id=\"item.toLowerCase()\"></component>\n    </div>\n    <FooterNav></FooterNav>\n</template>\n\n<script>\n// import {toRaw} from \"vue\";\nexport default {\n  name: 'App',\n  components: {\n  },\n  props: {\n    scrollPos: {\n      type: Number,\n      default: 0\n    }\n  },\n  watch: {\n    scrollPos(scrollPos) {\n      // console.log(\"scrollPos\", scrollPos);\n      if (this.positions.filter(item => scrollPos >= item).length > 0) {\n        document.getElementById(this.components[this.positions.filter(item => scrollPos >= item).length - 1])\n      }\n    }\n  },\n  methods: {\n  },\n  data() {\n    return {\n      components: [\n        'HomepageBanner',\n        'TextandFadedBG',\n        'GoldSquareSection',\n        'CarouselText',\n        'ImagesGallery'\n      ],\n      positions: []\n    }\n  },\n  mounted() {\n    this.$nextTick(() => {\n      this.positions = [...this.components].map((id) => {\n        let component = document.getElementById(id.toLowerCase());\n        return (component.getBoundingClientRect().top - ((window.innerHeight - component.getBoundingClientRect().height) / 2)) + window.scrollY;\n      })\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\">\n@import \"fonts.scss\";\n@import \"styles.scss\";\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}