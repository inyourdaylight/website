{"ast":null,"code":"import HeaderNavScreen from './components/HeaderNavScreen.vue';\nimport HeaderNav from './components/HeaderNav.vue';\nexport default {\n  name: 'IndexIndex',\n  props: {},\n  components: {\n    HeaderNavScreen,\n    HeaderNav\n  },\n  watch: {},\n  methods: {\n    clickEvents(e) {\n      switch (e.target.id) {\n        case \"hamburgerMenu\":\n          {\n            this.showNav = !this.showNav;\n            break;\n          }\n      }\n    },\n    scroll() {\n      this.scrollPos = window.scrollY;\n    }\n  },\n  data() {\n    return {\n      showNav: false,\n      scrollLimit: 0,\n      scrollPos: 0\n    };\n  },\n  mounted() {\n    // Defining only one click event listener here, to prevent possible memory leaks\n    window.addEventListener(\"click\", this.clickEvents);\n    this.$nextTick(() => {\n      window.addEventListener(\"scroll\", this.scroll);\n      this.scrollLimit = document.getElementById(\"page\").getBoundingClientRect().height - window.innerHeight;\n    });\n  }\n};","map":{"version":3,"mappings":"AAkBA,OAAOA,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE,CACP,CAAC;EACDC,UAAU,EAAE;IACVJ,eAAe;IACfC;EACF,CAAC;EACDI,KAAK,EAAE,CAAC,CAAC;EACTC,OAAO,EAAE;IACPC,WAAW,CAACC,CAAC,EAAE;MACb,QAAOA,CAAC,CAACC,MAAM,CAACC,EAAE;QAChB,KAAK,eAAe;UAAE;YACpB,IAAI,CAACC,OAAM,GAAI,CAAC,IAAI,CAACA,OAAO;YAC5B;UACF;MAAA;IAEJ,CAAC;IACDC,MAAM,GAAG;MACP,IAAI,CAACC,SAAQ,GAAIC,MAAM,CAACC,OAAO;IACjC;EACF,CAAC;EACDC,IAAI,GAAG;IACL,OAAO;MACLL,OAAO,EAAE,KAAK;MACdM,WAAW,EAAE,CAAC;MACdJ,SAAS,EAAE;IACb;EACF,CAAC;EACDK,OAAO,GAAG;IACP;IACAJ,MAAM,CAACK,gBAAgB,CAAC,OAAO,EAAE,IAAI,CAACZ,WAAW,CAAC;IACnD,IAAI,CAACa,SAAS,CAAC,MAAM;MACnBN,MAAM,CAACK,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACP,MAAM,CAAC;MAC9C,IAAI,CAACK,WAAU,GAAII,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,qBAAqB,EAAE,CAACC,MAAK,GAAIV,MAAM,CAACW,WAAW;IACxG,CAAC;EAEH;AACF","names":["HeaderNavScreen","HeaderNav","name","props","components","watch","methods","clickEvents","e","target","id","showNav","scroll","scrollPos","window","scrollY","data","scrollLimit","mounted","addEventListener","$nextTick","document","getElementById","getBoundingClientRect","height","innerHeight"],"sourceRoot":"","sources":["/Users/rebj/Sites/inyourdaylight/src/Index.vue"],"sourcesContent":["<template>\n  <div class=\"index\" id=\"app\">\n    <div class=\"gradient\"></div>\n\n    <HeaderNav \n    :scrollLimit=\"scrollLimit\" \n    :scrollPos=\"scrollPos\"\n    :showNav=\"showNav\"></HeaderNav>\n    <transition appear name=\"header-nav-screen\">\n      <HeaderNavScreen v-show=\"showNav\" :showNav=\"showNav\"></HeaderNavScreen>\n    </transition>\n\n    <router-view class=\"page\" id=\"page\"></router-view>\n\n  </div>\n</template>\n\n<script>\nimport HeaderNavScreen from './components/HeaderNavScreen.vue';\nimport HeaderNav from './components/HeaderNav.vue';\nexport default {\n  name: 'IndexIndex',\n  props: {\n  },\n  components: {\n    HeaderNavScreen,\n    HeaderNav \n  },\n  watch: {},\n  methods: {\n    clickEvents(e) {\n      switch(e.target.id) {\n        case \"hamburgerMenu\": {\n          this.showNav = !this.showNav;\n          break;\n        }\n      }\n    },\n    scroll() {\n      this.scrollPos = window.scrollY;\n    },\n  },\n  data() {\n    return {\n      showNav: false,\n      scrollLimit: 0,\n      scrollPos: 0,\n    }\n  },\n  mounted() {\n     // Defining only one click event listener here, to prevent possible memory leaks\n     window.addEventListener(\"click\", this.clickEvents);\n    this.$nextTick(() => {\n      window.addEventListener(\"scroll\", this.scroll);\n      this.scrollLimit = document.getElementById(\"page\").getBoundingClientRect().height - window.innerHeight;\n    })\n   \n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"scss\">\n@import \"./styles.scss\";\n@import \"./fonts.scss\";\np, li, div {\n    font-family: $body;\n    color: white;\n    margin-top: 0;\n}\n.gradient {\n  position: absolute;\n  width: 100vw;\n  height: 479px;\n  background: linear-gradient(180deg, rgba(50, 62, 71, 0) 0%, #323E47 94.79%);\n  transform: rotate(-180deg);\n  z-index: 0;\n  pointer-events: none;\n}\n#app {\n  background-color: $darkblue;\n}\n.page {\n  z-index: 1;\n  position: relative;\n}\n.header-nav-screen  {\n  width: 100vw;\n  transition: all 1000ms cubic-bezier(0.355, 0.005, 0.240, 0.990); /* custom */\n  transition-timing-function: cubic-bezier(0.355, 0.005, 0.240, 0.990); /* custom */\n}\n.header-nav-screen-enter-to {\n  width: 100vw !important;\n}\n.header-nav-screen-enter-active {\n  width: 0vw;\n}\n.header-nav-screen-leave-to {\n  width: 0vw;\n}\n\n.page {\n  position: relative;\n  z-index: 2;\n  min-height: 200vh;\n}\nh1 {\n    font-family: $heading;\n    text-transform: uppercase;\n}\nh3 {\n    font-family: $headinglight;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}