{"ast":null,"code":"import industries from \"../industries.js\";\nimport Email from \"../smtp.js\";\nexport default {\n  name: 'CDAPApplication',\n  props: {\n    clickEvents: {\n      type: Object,\n      default: () => {}\n    }\n  },\n  watch: {\n    clickEvents: {\n      handler(clickEvent) {\n        switch (clickEvent.target.className) {\n          case \"dropdown closed\":\n            {\n              clickEvent.target.getElementsByClassName(\"selection\")[0].style.maxHeight = '400px';\n              clickEvent.target.className = \"dropdown open\";\n              clickEvent.target.getElementsByClassName(\"arrow\")[0].className = \"arrow up\";\n              break;\n            }\n          case \"dropdown open\":\n            {\n              clickEvent.target.getElementsByClassName(\"selection\")[0].style.maxHeight = '0px';\n              clickEvent.target.className = \"dropdown closed\";\n              clickEvent.target.getElementsByClassName(\"arrow\")[0].className = \"arrow\";\n              break;\n            }\n          case \"option industry\":\n            {\n              clickEvent.target.parentElement.style.maxHeight = '0px';\n              clickEvent.target.parentElement.parentElement.className = \"dropdown closed\";\n              this.activeIndustry = clickEvent.target.innerHTML;\n              clickEvent.target.parentElement.parentElement.getElementsByClassName(\"arrow\")[0].className = \"arrow\";\n              break;\n            }\n          case \"option application\":\n            {\n              clickEvent.target.parentElement.style.maxHeight = '0px';\n              clickEvent.target.parentElement.parentElement.className = \"dropdown closed\";\n              this.application = clickEvent.target.innerHTML;\n              break;\n            }\n        }\n      }\n    }\n  },\n  methods: {\n    submitForm() {\n      let data = `\n            Name: ${this.$refs.name.value}<br/>\n            Company Name: ${this.$refs.company_name.value}<br/>\n            Business: ${this.$refs.business.value}<br/>\n            Phone: ${this.$refs.phone.value}<br/>\n            Email: ${this.$refs.email.value}<br/>\n            CDAP Application previously started: ${this.$refs.app.value}<br/>\n            Referred by: ${this.$refs.refer.value}<br/>\n        `;\n      let obj = {\n        // 73483dcc-cbd2-4610-8094-b369236f5c0c\n      };\n      Email.send(data).then(() => {\n        console.log(\"sent!\");\n      });\n    },\n    doIndustrySearch(e) {\n      // Show dropdown\n      e.target.parentElement.parentElement.getElementsByClassName(\"selection\")[0].style.maxHeight = '400px';\n      e.target.parentElement.className = \"dropdown open\";\n      e.target.parentElement.getElementsByClassName(\"arrow\")[0].className = \"arrow up\";\n      // Current value is what user is typing in\n      this.activeIndustry = e.target.value;\n      // If not results found, display that in the dropdown\n      if ([...industries.list].filter(i => i.toLowerCase().includes(e.target.value.toLowerCase())).length === 0) {\n        this.industries = {\n          list: ['No results found.']\n        };\n        return;\n      }\n      this.industries = {\n        list: [...industries.list].filter(i => i.toLowerCase().includes(this.activeIndustry.toLowerCase()))\n      };\n    },\n    handleChange(clickEvent) {\n      if ([...industries.list].indexOf(this.activeIndustry) < 0) {\n        this.activeIndustry = null;\n        this.industries = industries;\n      }\n      clickEvent.target.parentElement.getElementsByClassName(\"selection\")[0].style.maxHeight = '0px';\n      clickEvent.target.parentElement.className = \"dropdown closed\";\n      clickEvent.target.parentElement.getElementsByClassName(\"arrow\")[0].className = \"arrow\";\n    }\n  },\n  data() {\n    return {\n      industries: industries,\n      activeIndustry: null,\n      application: \"No\"\n    };\n  },\n  mounted() {\n    this.activeIndustry = this.industries.list[0];\n  }\n};","map":{"version":3,"mappings":"AAsEA,OAAOA,UAAS,MAAO,kBAAkB;AACzC,OAAOC,KAAI,MAAO,YAAY;AAC9B,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAK,EAAE;IACLC,WAAW,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE,MAAM,CAAC;IACpB;EACF,CAAC;EACDC,KAAK,EAAE;IACLJ,WAAW,EAAE;MACTK,OAAO,CAACC,UAAU,EAAE;QAChB,QAAOA,UAAU,CAACC,MAAM,CAACC,SAAS;UAC9B,KAAK,iBAAiB;YAAE;cACpBF,UAAU,CAACC,MAAM,CAACE,sBAAsB,CAAC,WAAW,CAAC,CAAC,CAAC,CAAC,CAACC,KAAK,CAACC,SAAQ,GAAI,OAAO;cAClFL,UAAU,CAACC,MAAM,CAACC,SAAQ,GAAI,eAAe;cAC7CF,UAAU,CAACC,MAAM,CAACE,sBAAsB,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,CAACD,SAAQ,GAAI,UAAU;cAC3E;YACJ;UACA,KAAK,eAAe;YAAE;cAClBF,UAAU,CAACC,MAAM,CAACE,sBAAsB,CAAC,WAAW,CAAC,CAAC,CAAC,CAAC,CAACC,KAAK,CAACC,SAAQ,GAAI,KAAK;cAChFL,UAAU,CAACC,MAAM,CAACC,SAAQ,GAAI,iBAAiB;cAC/CF,UAAU,CAACC,MAAM,CAACE,sBAAsB,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,CAACD,SAAQ,GAAI,OAAO;cACxE;YACJ;UACA,KAAK,iBAAiB;YAAE;cACpBF,UAAU,CAACC,MAAM,CAACK,aAAa,CAACF,KAAK,CAACC,SAAQ,GAAI,KAAK;cACvDL,UAAU,CAACC,MAAM,CAACK,aAAa,CAACA,aAAa,CAACJ,SAAQ,GAAI,iBAAiB;cAC3E,IAAI,CAACK,cAAa,GAAIP,UAAU,CAACC,MAAM,CAACO,SAAS;cACjDR,UAAU,CAACC,MAAM,CAACK,aAAa,CAACA,aAAa,CAACH,sBAAsB,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,CAACD,SAAQ,GAAI,OAAO;cACpG;YACJ;UACA,KAAK,oBAAoB;YAAE;cACvBF,UAAU,CAACC,MAAM,CAACK,aAAa,CAACF,KAAK,CAACC,SAAQ,GAAI,KAAK;cACvDL,UAAU,CAACC,MAAM,CAACK,aAAa,CAACA,aAAa,CAACJ,SAAQ,GAAI,iBAAiB;cAC3E,IAAI,CAACO,WAAU,GAAIT,UAAU,CAACC,MAAM,CAACO,SAAS;cAC9C;YACJ;QAAA;MAGR;IACJ;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,UAAU,GAAG;MACT,IAAIC,IAAG,GAAI;oBACC,IAAI,CAACC,KAAK,CAACrB,IAAI,CAACsB,KAAM;4BACd,IAAI,CAACD,KAAK,CAACE,YAAY,CAACD,KAAM;wBAClC,IAAI,CAACD,KAAK,CAACG,QAAQ,CAACF,KAAM;qBAC7B,IAAI,CAACD,KAAK,CAACI,KAAK,CAACH,KAAM;qBACvB,IAAI,CAACD,KAAK,CAACK,KAAK,CAACJ,KAAM;mDACO,IAAI,CAACD,KAAK,CAACM,GAAG,CAACL,KAAM;2BAC7C,IAAI,CAACD,KAAK,CAACO,KAAK,CAACN,KAAM;SACzC;MACD,IAAIO,GAAE,GAAI;QACN;MAAA,CACJ;MACA9B,KAAK,CAAC+B,IAAI,CAACV,IAAI,CAAC,CAACW,IAAI,CAAC,MAAM;QACxBC,OAAO,CAACC,GAAG,CAAC,OAAO,CAAC;MACxB,CAAC;IACL,CAAC;IACDC,gBAAgB,CAACC,CAAC,EAAE;MAChB;MACAA,CAAC,